<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <navigation-rule>
        
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>cadCondomino</from-outcome>
            <to-view-id>/condominio/cadastrar.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>listarCondomino</from-outcome>
            <to-view-id>/condominio/listar.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>cadMorador</from-outcome>
            <to-view-id>/morador/cadastrar.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>cadFuncionario</from-outcome>
            <to-view-id>/funcionario/cadastrar.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>listarMorador</from-outcome>
            <to-view-id>/morador/listar.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>listarFuncionario</from-outcome>
            <to-view-id>/funcionario/listar.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>cadLocal</from-outcome>
            <to-view-id>/areacomun/cadastrar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
